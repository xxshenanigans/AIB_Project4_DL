      "source": [
        "# 문자 벡터화\n",
        "\n",
        "def bin(name):\n",
        "\n",
        "    chars = []\n",
        "\n",
        "    for i in alphabets:\n",
        "        count = 0\n",
        "        for j in name:\n",
        "            if i == j:\n",
        "                count +=1\n",
        "        chars.append(count)\n",
        "\n",
        "    return chars\n",
        "\n",
        "train_copy['bin'] = train_copy['IDENTITY'].apply(lambda x: bin(x)) "
      ],
      "execution_count": null,
      "outputs": []
