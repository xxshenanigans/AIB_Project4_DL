        "# 유사도 측정\n",
        "\n",
        "def cos_sim(A, B):\n",
        "       return dot(A, B)/(norm(A)*norm(B))\n",
        "\n",
        "cosine_sim = []\n",
        "for i in range(len(train_copy)):\n",
        "    cosine_sim.append(cos_sim(text, train_copy['bin'][i]))"
